@model MusicStore.Web.Models.UserStatisticViewModel

@{
    ViewBag.Title = "DisplayUserStatistic";
}

<h2>DisplayUserStatistic</h2>

<div>
    <h4>UserStatisticViewModel</h4>
    <div class="row">
        <div class="col-lg-6">
            @Html.DisplayNameFor(model => model.TotalNumberOfSongs)
        </div>

        <div class="col-lg-6">
            @Html.DisplayFor(model => model.TotalNumberOfSongs)
        </div>
    </div>
    
    <div class="row">
        <div class="col-lg-6">
            @Html.DisplayNameFor(model => model.TotalSpentMoney)
        </div>

        <div class="col-lg-6">
            @Html.DisplayFor(model => model.TotalSpentMoney)
        </div>
    </div>
</div>
